<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/content_dashboard_empty"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <me.citrafa.asistenkuliahku.CustomWidget.TVLatoFontMedium
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/empty1"
        android:textStyle="bold"
        android:layout_centerInParent="true"
        android:text="Tidak ada jadwal"/>
    <me.citrafa.asistenkuliahku.CustomWidget.TVLatoFontRegular
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/empty1"
        android:layout_centerInParent="true"
        android:id="@+id/empty2"
        android:text="Kamu Bisa Membuatnya dengan tombol +"/>
    <android.support.v7.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_list"
        android:layout_above="@id/empty1"
        android:layout_centerInParent="true"/>
</RelativeLayout>